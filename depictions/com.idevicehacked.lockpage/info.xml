<package>
	<id>com.idevicehacked.lockpage</id>
	<name>LockPage</name>
	<version>1.0.1</version>
	<compatibility>
		<firmware>
			<maxiOS>11.4.1</maxiOS>
            <miniOS>11.0</miniOS>
			<otherVersions>unconfirmed</otherVersions>
			<!--
			for otherVersions, you can put either unsupported or unconfirmed
			-->
