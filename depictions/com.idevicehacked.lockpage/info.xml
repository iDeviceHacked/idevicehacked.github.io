            <description>Preferences</description>
            <image>settings.png</image>
        </screenshot>
    </screenshots>
	<changelog>
        <change>Added Custom (Locked) Text!</change>
	</changelog>
	<links>
        <link>
            <name>Website!</name>
            <url>https://www.idevicehacked.com</url>
            <iconclass>fa fa-desktop</iconclass>
        </link>
    </links>
</package>
